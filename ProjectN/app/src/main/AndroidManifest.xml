<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sora.projectn">

    <!-- 网络访问 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 外部存储卡的读写权限 -->
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".model.Activity.WelcomeActivity_Old"></activity>
        <activity android:name=".model.Activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".model.Activity.MainActivity"></activity>
        <activity android:name=".model.Activity.TestActivity"></activity>
        <activity android:name=".model.Activity.TeamListActivity_Old"></activity>
        <activity android:name=".model.Activity.TeamActivity"></activity>
        <activity android:name=".model.Activity.TeamListActivity"></activity>
        <activity android:name=".model.Activity.MatchActivity"></activity>
        <activity android:name=".model.Activity.RankActivity"></activity>
        <activity android:name=".model.Activity.TeamCombatListActivity"></activity>
        <activity android:name=".model.Activity.TeamCombatActivity"></activity>
        <activity android:name=".model.Activity.MatchSearchActivity"></activity>
        <activity android:name=".model.Activity.TeamSelectActivity"></activity>
        <activity android:name=".model.Activity.PlayerInfoActivity"></activity>

        <service android:name=".gc.Service.ScrapeService"></service>

        <activity
            android:name=".model.Activity.SearchPlayerActivity"
            android:label="@string/title_activity_search_player"></activity>
    </application>

</manifest>
